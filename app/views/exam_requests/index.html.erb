<p id="notice"><%= notice %></p>

<h1>Listing Exam Requests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exam_requests.each do |exam_request| %>
      <tr>
        <td><%= link_to 'Show', exam_request %></td>
        <td><%= link_to 'Edit', edit_exam_request_path(exam_request) %></td>
        <td><%= link_to 'Destroy', exam_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exam request', new_exam_request_path %>

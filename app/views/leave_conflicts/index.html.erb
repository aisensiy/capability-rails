<p id="notice"><%= notice %></p>

<h1>Listing Leave Conflicts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @leave_conflicts.each do |leave_conflict| %>
      <tr>
        <td><%= link_to 'Show', leave_conflict %></td>
        <td><%= link_to 'Edit', edit_leave_conflict_path(leave_conflict) %></td>
        <td><%= link_to 'Destroy', leave_conflict, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Leave conflict', new_leave_conflict_path %>
